{
    "name": "yaelliethy/wildqueue",
    "description": "A Laravel package for dynamically managing queue workers.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Youssef Elliethy",
            "email": "y.a.elliethy@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/queue": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/bus": "^9.0|^10.0|^11.0|^12.0",
        "symfony/process": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "WildQueue\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WildQueue\\WildQueueServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 
